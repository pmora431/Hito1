<div class="container">
<div class="row mx-auto p-3">
<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>
<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 col-xl-4">

<% @User.each do |user| %>
<!-- code start -->
<div class="twPc-div">
    <a class="twPc-bg twPc-block"></a>

	<div>
		<div class="twPc-button">
            <button type="button" class="btn bg-primary text-light btn-sm fab fa-twitter"> Seguir</button>
		</div>

		<p class="twPc-avatarLink">
            <%= image_tag(user.picture, class: 'card-img-top') %>
		</p>

		<div class="twPc-divUser">
			<div class="twPc-divName">
				<%= user.username %>
			</div>
			<span>
				@<%= user.username %>
			</span>
		</div>
	</div>
</div>
<br>
<!-- code end -->
<% end %>

</div>
<div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 col-xl-4"></div>  
</div>
</div>